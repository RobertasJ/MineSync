    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "custom",
            "name": "Custom launch",
            "targetCreateCommands": [
                "target create ${workspaceFolder}/sync-testing/sync.exe"
            ],
            "processCreateCommands": [
                "process launch"
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'rlib'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=rlib"
                ],
                "filter": {
                    "name": "rlib",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rlib'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rlib",
                    "--package=rlib"
                ],
                "filter": {
                    "name": "rlib",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'rlib'",
